DIRS=misc httpd
AUTOCRAP=Makefile httpd/Makefile misc/Makefile include/config.h config.status \
	config.log config.cache

all:
	rm -f httpd/httpd
	for i in $(DIRS); do cd $$i && $(MAKE) ; cd .. ; done

clean:
	for i in $(DIRS); do cd $$i && $(MAKE) $@ ; cd .. ; done

ultrabrite:
	for i in $(DIRS); do cd $$i && $(MAKE) clean ; cd .. ; done
	-rm -f $(AUTOCRAP)
