.SUFFIXES: .m .o

CFLAGS=-I../include -g
AR=@AR@ cr
RANLIB=@RANLIB@
OBJC=@CC@
LIBS=@LIBS@
LDFLAGS=$(LIBS)
EXES=test
MISCOBJS=dObject.o dString.o dThread.o
SOURCE=dObject.m dString.m dThread.m
JUNK=$(EXES) *.o *.a

libdustin.a: $(MISCOBJS)
	$(AR) $@ $(MISCOBJS)
	$(RANLIB) $@

clean:
	-rm -f $(JUNK)

.m:	$@.o
	$(OBJC) -o $> $< $(LDFLAGS)

.o:
	$(OBJC) -o $> $< $(LDFLAGS)

.m.o:
	$(OBJC) $(CFLAGS) -c $<
