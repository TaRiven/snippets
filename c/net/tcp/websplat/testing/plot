# Plot the data from analyze.
# $Id: plot,v 1.1 1999/09/01 21:24:30 dustin Exp $

set title "Response Time By Number of Clients"
set data style linespoints
set grid
# set boxwidth -3
set xlabel "Number of Simultaneous Orders"
set ylabel "Seconds"
plot \
	'analyzed.dat' using 1:6 t 'Max Seconds',\
	'analyzed.dat' using 1:3 t 'Avg Seconds',\
	'analyzed.dat' using 1:5 t 'Min Seconds'
pause -1 "Press Enter When Finished"
