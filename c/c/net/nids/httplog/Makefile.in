DEFS=@DEFS@
CFLAGS=@CFLAGS@ $(DEFS) -DMYMALLOC
LDFLAGS=@LDFLAGS@ @LIBS@

OBJS=httplog.o mymalloc.o

httplog: $(OBJS)
	$(CC) -o $@ httplog.o mymalloc.o $(LDFLAGS)

clean:
	rm -f httplog $(OBJS)

distclean: clean
	rm -f config.log config.cache config.status Makefile

httplog.o: httplog.c httplog.h
