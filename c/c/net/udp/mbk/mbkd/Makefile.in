SHELL=/bin/sh
CC=@CC@
YACC=@YACC@
LEX=@LEX@
LINT=@LINT@
INCLUDES=-I@top_srcdir@ -I@top_srcdir@/readconfig
CFLAGS=@CFLAGS@ $(INCLUDES)

LIBS=@top_srcdir@/readconfig/readconfig.a @LIBS@
LDFLAGS=@LDFLAGS@ $(LIBS)

OBJS=main.o nettools.o sockets.o utility.o hash.o md5.o mymalloc.o
SRCS=main.c nettools.c sockets.c utility.c hash.c md5.c mymalloc.c

all: mbkd

mbkd: $(OBJS)
	$(CC) -o $@ $(OBJS) $(LDFLAGS)

clean:
	rm -f $(OBJS) mbkd
