#!/bin/sh
# $Id: ten,v 1.1 1997/12/12 18:36:30 dustin Exp $

. /home/monitor/lib/shellconf

# normal scan things
$HOMEBIN/runscans &

# Other info
$HOME/stats/doit &

# netmon 2
$HOMEBIN/netmon2
chmod 644 $HOME/lib/plotdata

$HOMEBIN/geticsorder &

# State logs
$HOMEBIN/statelog &

# get process lists
$HOMEBIN/getpslist arthur tessa ics.ic3.com &
$HOMEBIN/getdisklog arthur tessa ics.ic3.com &

# goldengate
$HOMEBIN/goldengate &

wait
